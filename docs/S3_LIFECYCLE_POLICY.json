{
  "Rules": [
    {
      "Id": "InputFilesLifecycle",
      "Status": "Enabled",
      "Filter": {
        "Prefix": "service/input/"
      },
      "Transitions": [
        {
          "Days": 30,
          "StorageClass": "STANDARD_IA",
          "Comment": "30日後に低頻度アクセスストレージへ移行"
        },
        {
          "Days": 90,
          "StorageClass": "GLACIER_IR",
          "Comment": "90日後にGlacier Instant Retrievalへ移行"
        }
      ],
      "Expiration": {
        "Days": 365,
        "Comment": "365日後に自動削除"
      }
    },
    {
      "Id": "SupportInputFilesLifecycle",
      "Status": "Enabled",
      "Filter": {
        "Prefix": "support/input/"
      },
      "Transitions": [
        {
          "Days": 30,
          "StorageClass": "STANDARD_IA"
        },
        {
          "Days": 90,
          "StorageClass": "GLACIER_IR"
        }
      ],
      "Expiration": {
        "Days": 365
      }
    },
    {
      "Id": "OutputFilesLifecycle",
      "Status": "Enabled",
      "Filter": {
        "Prefix": "service/output/"
      },
      "Transitions": [
        {
          "Days": 90,
          "StorageClass": "STANDARD_IA",
          "Comment": "出力ファイルは長めに即座アクセス可能に保持"
        }
      ],
      "Expiration": {
        "Days": 730,
        "Comment": "2年後に削除"
      }
    },
    {
      "Id": "SupportOutputFilesLifecycle",
      "Status": "Enabled",
      "Filter": {
        "Prefix": "support/output/"
      },
      "Transitions": [
        {
          "Days": 90,
          "StorageClass": "STANDARD_IA"
        }
      ],
      "Expiration": {
        "Days": 730
      }
    }
  ]
}

